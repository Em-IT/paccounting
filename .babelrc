{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {"node": "current"}
      }
    ],
    [
      "@babel/preset-react", {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "babel-plugin-twin",
    "babel-plugin-macros",
    "babel-plugin-styled-components"
  ]
}